    const num = (Number(1670).toString(2));
const binary = num.split("");
const counts = {};
console.log(binary);   

binary.forEach(zero => {
    if(counts[zero]){
        counts[zero]++;
    }
    else{
        counts[zero] = 1;
    }
});
console.log(counts); 